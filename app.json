{
  "expo": {
    "name": "Fokin Weater App",
    "slug": "fokin-weater",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#005aeb"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "kr.hyungseok.weather.fokin"
    },
    "description": "Learning React Native by Building Foking Weather App",
    "githubUrl": "https://github.com/jhs851/fokin-weather"
  }
}